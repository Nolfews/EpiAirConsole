{
  "scripts": {
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm run start",
    "backend:dev": "cd backend && npm run dev",
    "socket:frontend": "node frontend/scripts/runSocket.js",
    "socket:mobile": "node mobile/scripts/runSocket.js",
    "build:mobile:scss": "sass --no-source-map mobile/scss/mobile.scss mobile/public/mobile.css --style=compressed",
    "watch:mobile:scss": "sass --no-source-map --watch mobile/scss/mobile.scss:mobile/public/mobile.css --style=expanded",
    "build:frontend:scss": "sass --no-source-map frontend/scss/frontend.scss frontend/public/frontend.css --style=compressed",
    "watch:frontend:scss": "sass --no-source-map --watch frontend/scss/frontend.scss:frontend/public/frontend.css --style=expanded"
  },
  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "sass": "^1.92.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "phaser": "^3.90.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
  "typescript-eslint": "^8.44.0",
  "ts-node-dev": "^2.0.0"
  }
}
